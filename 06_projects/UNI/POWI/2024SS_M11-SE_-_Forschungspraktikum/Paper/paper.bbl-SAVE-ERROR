    \entry{manin1997}{book}{}{}
      \name{author}{1}{}{%
        {{un=0,uniquepart=base,hash=53330ddde0011db6cb8733d604c66e38}{%
           family={Manin},
           familyi={M\bibinitperiod},
           given={Bernard},
