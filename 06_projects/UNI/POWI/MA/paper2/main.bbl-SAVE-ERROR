      \strng{namehash}{aad1333d0afdfd67f1c8efcce93bb836}
      \strng{fullhash}{aad1333d0afdfd67f1c8efcce93bb836}
      \strng{fullhashraw}{aad1333d0afdfd67f1c8efcce93bb836}
      \strng{bibnamehash}{aad1333d0afdfd67f1c8efcce93bb836}
      \strng{authorbibnamehash}{aad1333d0afdfd67f1c8efcce93bb836}
