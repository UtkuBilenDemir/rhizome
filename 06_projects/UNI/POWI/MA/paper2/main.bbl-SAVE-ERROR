    \entry{Cheney2011}{article}{}{}
      \name{author}{1}{}{%
        {{un=0,uniquepart=base,hash=0531281fb30c64dc5a782934a5c20450}{%
           family={{Cheney-Lippold}},
           familyi={C\bibinitperiod},
           given={John},
           giveni={J\bibinitperiod},
           givenun=0}}%
      }
      \strng{namehash}{0531281fb30c64dc5a782934a5c20450}
      \strng{fullhash}{0531281fb30c64dc5a782934a5c20450}
      \strng{fullhashraw}{0531281fb30c64dc5a782934a5c20450}
      \strng{bibnamehash}{0531281fb30c64dc5a782934a5c20450}
      \strng{authorbibnamehash}{0531281fb30c64dc5a782934a5c20450}
      \strng{authornamehash}{0531281fb30c64dc5a782934a5c20450}
