    \entry{eloff2021}{article}{}{}
      \name{author}{1}{}{%
        {{un=0,uniquepart=base,hash=b5ff69978f7e5c32332bdf613402ad43}{%
           family={Eloff},
           familyi={E\bibinitperiod},
           given={Aragorn},
           giveni={A\bibinitperiod},
           givenun=0}}%
      }
      \strng{namehash}{b5ff69978f7e5c32332bdf613402ad43}
      \strng{fullhash}{b5ff69978f7e5c32332bdf613402ad43}
      \strng{fullhashraw}{b5ff69978f7e5c32332bdf613402ad43}
      \strng{bibnamehash}{b5ff69978f7e5c32332bdf613402ad43}
      \strng{authorbibnamehash}{b5ff69978f7e5c32332bdf613402ad43}
      \strng{authornamehash}{b5ff69978f7e5c32332bdf613402ad43}
      \strng{authorfullhash}{b5ff69978f7e5c32332bdf613402ad43}
