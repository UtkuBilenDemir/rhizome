    \entry{amoore2023}{article}{}{}
      \name{author}{1}{}{%
        {{un=0,uniquepart=base,hash=4b3488aaa126f9ef741ea766698b9f28}{%
           family={Amoore},
           familyi={A\bibinitperiod},
           given={Louise},
